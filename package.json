{
  "name": "light-mapper",
  "version": "1.1.6",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage npm run test:only",
    "build": "cross-env BABEL_ENV=production tsc --module commonjs",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hajekj14/light-mapper.git"
  },
  "keywords": [
    "mapper",
    "object mapper",
    "light",
    "light-mapper",
    "typescript"
  ],
  "author": "Hajek Jiri <hajekj14@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flexdinesh/npm-module-boilerplate/issues"
  },
  "homepage": "https://github.com/flexdinesh/npm-module-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  }
}
